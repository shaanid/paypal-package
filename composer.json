{
    "name": "shaanid/paypal-package",
    "description": "A PayPal integration package for Laravel.",
    "keywords": ["laravel", "paypal", "payment"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "srmklive/paypal": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Shaanid\\PayPal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shaanid\\PayPal\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shaanid\\PayPal\\PayPalServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
